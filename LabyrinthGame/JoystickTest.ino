/**/#define VR_X_PIN A0#define VR_Y_PIN A1#define SW_PIN A2void setup() {        pinMode(VR_X_PIN, INPUT);    pinMode(VR_Y_PIN, INPUT);    pinMode(SW_PIN, INPUT);        Serial.begin(9600);}void loop() {    Serial.print("VRx: ");    Serial.print(analogRead(VR_X_PIN));  Serial.print("\tVRy: ");  Serial.print(analogRead(VR_Y_PIN));    Serial.print("\tSW: ");  Serial.println(analogRead(SW_PIN));      //Serial.println("1");}